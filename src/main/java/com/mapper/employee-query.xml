<?xml version="1.0" encoding="utf-8" standalone ="no" ?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
    <entry key="getEverything">
        select
            *
        from
            employee
    </entry>
    <entry key="phonechange">
        update employee
        set
        PHONE =?
        where
            EMP_NAME =?


    </entry>
</properties>